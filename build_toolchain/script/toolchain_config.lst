#!/bin/bash

# Name: toolchain_config.lst
# Author: Peter Peng
# Ver: 0.1

export PRJROOT=$1
export TARGET=arm-linux-gnueabihf
export PREFIX=${PRJROOT}/tool-chain
export TARGET_PREFIX=${PREFIX}/${TARGET}
export GLIBC_TARGET_PREFIX=${TARGET_PREFIX}/libc
export PATH=${PREFIX}/bin:${PATH}
export ARCH=arm

export BINUTILS_VERSION=binutils-2.40
export GCC_VERSION=gcc-12.3.0
export GLIBC_VERSION=glibc-2.32
export KERNEL_VERSION=linux-4.9.84


